    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4"
